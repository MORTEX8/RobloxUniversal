-- Roblox External Script Loader
-- Fetches and executes scripts from GitHub

local function loadExternalScripts()
    -- URLs for the scripts
    local aimbotUrl = "https://raw.githubusercontent.com/MORTEX8/RobloxUniversal/refs/heads/main/AimbotLua"
    local visualsUrl = "https://raw.githubusercontent.com/MORTEX8/RobloxUniversal/refs/heads/main/VisualsLua"
    
    -- Function to fetch and execute a script
    local function fetchAndExecute(url, scriptName)
        local success, response = pcall(function()
            return game:HttpGetAsync(url, true)
        end)
        
        if success and response then
            local executeSuccess, errorMsg = pcall(function()
                loadstring(response)()
            end)
            
            if executeSuccess then
                print(scriptName.." loaded successfully!")
            else
                warn("Failed to execute "..scriptName..": "..tostring(errorMsg))
            end
        else
            warn("Failed to fetch "..scriptName..": "..tostring(response))
        end
    end
    
    -- Load Aimbot script
    fetchAndExecute(aimbotUrl, "Aimbot Script")
    
    -- Load Visuals script
    fetchAndExecute(visualsUrl, "Visuals Script")
end

-- Execute the loader with error protection
local success, err = pcall(loadExternalScripts)
if not success then
    warn("Loader failed: "..tostring(err))
end
